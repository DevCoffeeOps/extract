{
    "name": "extract",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "postinstall": "npx ts-patch install",
        "prebuild": "rm -rf dist/",
        "build": "tsc && tsc-alias && cp set_env.js dist/",
        "start": "npm run build && node dist/index.js",
        "go": "node --import tsx src/index.ts"
    },
    "dependencies": {
        "@prisma/client": "^5.17.0",
        "axios": "^1.7.3",
        "prisma": "^5.16.1",
        "ts-runtime-checks": "^0.6.1"
    },
    "devDependencies": {
        "@types/node": "^20.14.10",
        "ts-patch": "^3.2.1",
        "tsc-alias": "^1.8.10",
        "tsx": "^4.16.2",
        "typescript": "^5.5.4"
    }
}
